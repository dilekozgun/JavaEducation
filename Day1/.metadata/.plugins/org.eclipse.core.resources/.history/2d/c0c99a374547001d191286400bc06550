package sesliHarfler;

public class Main {

	public static void main(String[] args) {
		// mini proje : kalın ve ince sesli harfleri yazdırmak
		char harf = 'E';
		
		switch (harf) {
		case 'A':
		case 'I':
		case 'O':
		case 'U':{	
			System.out.println("Girilen harf kalın seslidir.");
		}
		case 'e':
		case 'i':
		case 'ö':
		case 'ü':{
			
		}
		
		
		default:
		}
		
		
		
		
		
		
		
		
		
		

	}

}
