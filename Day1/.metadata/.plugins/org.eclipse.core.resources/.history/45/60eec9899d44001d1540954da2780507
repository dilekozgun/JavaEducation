package recapDemo1;

public class Main {

	public static void main(String[] args) {
		//recapDemo
		
		int sayi1 =20;
		int sayi2 = 25;
		int sayi3 = 2;
		int enBuyuk;
		String enBuyukSayi = "En büyük sayı : ";
		
		if(sayi1>sayi2 && sayi1>sayi3) {
			enBuyuk = sayi1;
			System.out.println(enBuyukSayi + enBuyuk);
		}else if(sayi2>sayi1 && sayi2>sayi3) {
			enBuyuk = sayi2;
			System.out.println(enBuyukSayi + enBuyuk);
		}else {
			enBuyuk = sayi3;
			System.out.println(enBuyukSayi + enBuyuk);
		}
		
		

	}

}
