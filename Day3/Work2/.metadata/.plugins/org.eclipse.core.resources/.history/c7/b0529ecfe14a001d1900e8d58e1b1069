package overriding;

public class OgrenciKrediManager extends BaseKrediManager{
	public double hesapla(double tutar) { //BaseKrediManager daki hesapla fonksiyonunu override ettik. Bu işlemi yapabilmemiz için inheritance(kalıtım) ilişkisi gerekiyor.Yani extends etmeliyiz ki fonksiyon miras olarak çocuğa geçsin.
		return tutar * 1.10;
	}

}
