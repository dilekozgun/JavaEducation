package loop;

import java.util.Iterator;

public class Main {

	public static void main(String[] args) {
		// Loop
		
		//For
		
		for (int i = 2; i <= 10; i+=2) {
			System.out.println(i);
		}
		System.out.println("For Döngüsü Bitti");
		
		//While
		int i=2;
		while (i<10) {
			System.out.println(i);
			i+=2;
		}
		System.out.println("While Döngüsü Bitti");
		
		//Do-While
		int j =0;
		do{
			System.out.println(j);
		}while(j<10);
		
		
		
		
		
		
		
		
		

	}

}
